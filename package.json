{
  "name": "nodejs-template",
  "version": "1.0.0",
  "description": "A Node.js template with TypeScript, Express, Prisma, Docker, and Vitest",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,json --exec 'tsx src/server.ts'",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/server.js",
    "start:prod": "npm run build && node dist/server.js",
    "lint": "eslint --config eslint.config.js .",
    "lint:fix": "eslint --config eslint.config.js . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "audit": "npm audit --audit-level=high",
    "audit:fix": "npm audit fix --audit-level=high",
    "outdated": "npm outdated",
    "update": "npm update",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run format && npm run test",
    "prepare": "husky install || true",
    "docker:build": "docker build -t nodejs-template:latest .",
    "docker:run": "docker run -p 3000:3000 nodejs-template:latest",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "migrate:dev": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "prisma:generate": "prisma generate && prisma generate zod",
    "prisma:studio": "prisma studio",
    "docs": "typedoc --out docs src/",
    "docs:serve": "serve docs",
    "seed": "tsx prisma/seed.ts",
    "clean": "rimraf dist coverage docs",
    "preinstall": "npx only-allow npm",
    "postinstall": "prisma generate",
    "init": "tsx scripts/init.ts",
    "generate": "tsx scripts/generate.ts",
    "commit": "cz"
  },
  "volta": {
    "node": "22.14.0",
    "npm": "10.5.0"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "boilerplate",
    "docker",
    "eslint",
    "prettier",
    "vitest"
  ],
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{ts,js,json,md,yml}": "prettier --write"
  },
  "author": "Salton Arthur Massally <salton@mikashboks.com>>",
  "license": "MIT",
  "engines": {
    "node": ">22.0.0"
  },
  "dependencies": {
    "@keyv/redis": "^4.3.2",
    "@prisma/client": "^6.5.0",
    "cache-manager": "^6.4.1",
    "cacheable": "^1.8.9",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "decimal.js": "^10.5.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "gcp-metadata": "^6.1.1",
    "google-auth-library": "^9.15.1",
    "helmet": "^8.1.0",
    "keyv": "^5.3.2",
    "luxon": "^3.6.1",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "safe-stable-stringify": "^2.5.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.24.2",
    "zod-prisma-types": "^3.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/inquirer": "^9.0.7",
    "@types/luxon": "^3.4.2",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.14.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitest/coverage-v8": "^3.1.1",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "commitizen": "^4.3.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-zod": "^1.4.0",
    "file-type": "^20.4.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "inquirer": "^12.5.0",
    "lint-staged": "^15.5.0",
    "minimist": "^1.2.8",
    "nodemon": "^3.1.9",
    "ora": "^8.2.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "prisma": "^6.5.0",
    "prismock": "^1.35.3",
    "rimraf": "^6.0.1",
    "serve": "^14.2.1",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typedoc": "^0.28.1",
    "typescript": "^5.8.3",
    "validate-npm-package-name": "^6.0.0",
    "vitest": "^3.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikashboks/nodejs-template.git"
  },
  "bugs": {
    "url": "https://github.com/mikashboks/nodejs-template/issues"
  },
  "homepage": "https://github.com/mikashboks/nodejs-template#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
